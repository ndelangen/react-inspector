{
  "name": "react-inspector",
  "version": "4.0.0",
  "description": "Power of Browser DevTools inspectors right inside your React app",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/*",
            "*.spec.*",
            "__snapshots__/*",
            "__snapshots__/*",
            "*.snap",
            "stories/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
